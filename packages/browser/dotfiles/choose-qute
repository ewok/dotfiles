#!/bin/env bash

CMD=$(echo "Default|Work" | {{@@ bin @@}}/rofi_run -dmenu -i -l 20 -p "Qutebrowser session:" -sep "|")

case $CMD in
    Default)
        {{@@ bin @@}}/qutebrowser --target tab-bg-silent "$1"
        ;;
    Work)
        {{@@ bin @@}}/qutebrowser -r work --target tab-bg-silent "$1"
        ;;
esac
